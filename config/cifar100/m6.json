{
    "dataset": "cifar100",
    "num_classes": 10,
    "pretrained": null,
    "batch_size": 128,
    "num_epochs": 600,
    "feature_dim": 256,
    "lr": 0.01,
    "wd": 0.0001,
    "training_type": "stochastic+adversarial",
    "w_ct": 0.0,
    "w_at": 1.0,
    "attack": "pgd",
    "epsilon": 0.03,
    "var_type": "anisotropic",
    "reg_type": "wca+max_entropy",
    "device": "cuda:1",
    "output_path": {
        "stats": "output/stats/a2snn_cifar100_m6_lr=0.01_wd=0.0001",
        "models": "output/models/a2snn_cifar100_m6_lr=0.01_wd=0.0001"
    },
    "monte_carlo_runs": 50
}
